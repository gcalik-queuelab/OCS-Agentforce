    <decisions>
        <name>Are_Payment_Terms_Required</name>
        <label>Are Payment Terms Required</label>
        <locationX>446</locationX>
        <locationY>1982</locationY>
        <defaultConnectorLabel>Not Required</defaultConnectorLabel>
        <rules>
            <name>Missin</name>
            <conditionLogic>(1 OR 2) AND 3</conditionLogic>
            <conditions>
                <leftValueReference>recordId.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>4. Post-Submission</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recordId.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>7. Closed Won</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recordId.PaymentTermsDays__c</leftValueReference>
                <operator>EqualTo</operator>
            </conditions>
            <connector>
                <targetReference>Payment_Terms_Required_Screen</targetReference>
            </connector>
            <label>Payment Terms Required</label>
        </rules>
    </decisions>
    <decisions>
        <description>Is Stage 4. Post-Submission - this stage requires opp lines to be added. If Opp Lines are not found then stop the process and require user to add Opp Services. Otherwise continue.</description>
        <name>Are_Services_Required_Decision</name>
        <label>Opp Service Warning Required?</label>
        <locationX>248</locationX>
        <locationY>650</locationY>
        <defaultConnector>
            <targetReference>Description_Required_Decision</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No - Continue</defaultConnectorLabel>
        <rules>
            <name>Yes_Required_Show_Warning_Outcome</name>
            <conditionLogic>(1 OR 3) AND 2</conditionLogic>
            <conditions>
                <leftValueReference>recordId.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>4. Post-Submission</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recordId.Count_of_Opp_Lines__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recordId.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>3. Solution</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Opp_Services_Required_Warning</targetReference>
            </connector>
            <label>Yes - Show Warning</label>
        </rules>
    </decisions>
