    <decisions>
        <description>If Award Date is after Contract Start Date then prompt user to fix this.
If Contract Start Date is after Contract End Date then prompt user to fix this.</description>
        <name>Dates_Misaligned_Decision</name>
        <label>Dates Misaligned?</label>
        <locationX>446</locationX>
        <locationY>1574</locationY>
        <defaultConnector>
            <targetReference>Are_Payment_Terms_Required</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No - Skip</defaultConnectorLabel>
        <rules>
            <name>Yes_Fix_Dates_Outcome</name>
            <conditionLogic>(1 OR 2) AND (3 OR 4)</conditionLogic>
            <conditions>
                <leftValueReference>recordId.CloseDate</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>recordId.Contract_Start_Date__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recordId.Contract_Start_Date__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>recordId.Contract_End_Date__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recordId.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>3. Solution</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recordId.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>4. Post-Submission</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Date_Alignment_Screen</targetReference>
            </connector>
            <label>Yes - Fix Dates</label>
        </rules>
    </decisions>
