    <formulas>
        <description>Site Name formula.</description>
        <name>f_Site_Name</name>
        <dataType>String</dataType>
        <expression>LEFT(
/* Use Known As Account Name if available, otherwise use full Account Name*/
IF(ISBLANK({!recordId.Known_As__c}),{!recordId.Name},{!recordId.Known_As__c}) &amp;
/* Append Street Address to end of formula if populated*/
IF(ISBLANK({!vNewSite.Site_Address__Street__s}),NULL,&quot; - &quot; &amp; {!vNewSite.Site_Address__Street__s})
,80)
/* Limit Name to 80 Chars */</expression>
    </formulas>
    <formulas>
        <name>fcity</name>
        <dataType>String</dataType>
        <expression>INITCAP({!v_New_Site_Address.city})</expression>
    </formulas>
