<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contact_With_Meeting_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  !$Setup.OCS_Config_Settings_CSH__c.Validation_Rules_Disabled__c,
   RecordType.Name = &quot;External Debrief&quot;,
  NOT(ISBLANK(Meeting_Date__c)),
  ISBLANK(Contact__c)
)</errorConditionFormula>
    <errorDisplayField>Contact__c</errorDisplayField>
    <errorMessage>Contact Required when Meeting Date is set</errorMessage>
</ValidationRule>
