<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_Date_Before_End_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND( !$Setup.OCS_Config_Settings_CSH__c.Validation_Rules_Disabled__c, Start_Date__c  &gt;  End_Date__c )</errorConditionFormula>
    <errorDisplayField>End_Date__c</errorDisplayField>
    <errorMessage>Agreement Start Date must be before the Agreement End Date</errorMessage>
</ValidationRule>
