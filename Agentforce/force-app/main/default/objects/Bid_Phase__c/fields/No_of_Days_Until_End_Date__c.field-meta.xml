<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_of_Days_Until_End_Date__c</fullName>
    <externalId>false</externalId>
    <formula>IF(TODAY() &lt; End_Date__c, End_Date__c - TODAY(), 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>If the End Date has not already passed then calculate the number of days until the end date. If the End Date has already passed then return 0.</inlineHelpText>
    <label># Days Until End Date</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
