<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Case_Manager_Name_Mis_Match__c</fullName>
    <description>Identifies where Case Manager Name and Case Manager Lookup are not matched</description>
    <formula>AND(
    NOT(ISBLANK(Case_Manager_Lookup__r.Name)),
    NOT(ISBLANK(Case_Manager__c)),
    Case_Manager_Lookup__r.Name &lt;&gt; Case_Manager__c
)</formula>
    <label>Case Manager Name Mis-Match</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
