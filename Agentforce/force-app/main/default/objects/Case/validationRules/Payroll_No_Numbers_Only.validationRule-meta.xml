<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Payroll_No_Numbers_Only</fullName>
    <active>false</active>
    <errorConditionFormula>AND(!$Setup.OCS_Config_Settings_CSH__c.Validation_Rules_Disabled__c,
(REGEX(Employee_Payroll_No__c, &quot;^[0-9]*$&quot;))  
)</errorConditionFormula>
    <errorDisplayField>Employee_Payroll_No__c</errorDisplayField>
    <errorMessage>Only numbers are allowed [0-9]</errorMessage>
</ValidationRule>
