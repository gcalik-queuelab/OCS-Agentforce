<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contract_End_Date_must_be_after_Contract</fullName>
    <active>true</active>
    <description>AD-757 Contract End Date must be after Contract Start Date</description>
    <errorConditionFormula>AND(!$Setup.OCS_Config_Settings_CSH__c.Validation_Rules_Disabled__c,Contract_End_Date__c &lt; Contract_Start_Date__c)</errorConditionFormula>
    <errorDisplayField>Contract_End_Date__c</errorDisplayField>
    <errorMessage>Contract End Date must be after Contract Start Date</errorMessage>
</ValidationRule>
