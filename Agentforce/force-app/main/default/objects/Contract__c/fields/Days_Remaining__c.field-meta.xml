<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_Remaining__c</fullName>
    <description>AD-431 - Tracks the number of days between todays date and the contract end date</description>
    <externalId>false</externalId>
    <formula>IF(Contract_End_Date__c &gt; TODAY(),Contract_End_Date__c -  TODAY(),0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Days Remaining</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
