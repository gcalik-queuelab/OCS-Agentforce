<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Award_Date_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND
(
!$Setup.OCS_Config_Settings_CSH__c.Validation_Rules_Disabled__c,
ISNULL( Award_Date__c  )
)</errorConditionFormula>
