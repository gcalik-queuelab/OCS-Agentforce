<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Evaluate_Credit_Check__c</fullName>
    <description>To evaluate if a credit check has been completed or not based on credit score data and credit risk. The credit check performed If any of this field is not null.</description>
    <externalId>false</externalId>
    <formula>IF(
     (ISPICKVAL( Account__r.Credit_Risk__c , &quot;&quot;)   &amp;&amp;   ISNULL( Account__r.Latest_Credit_Score_Date__c )),
    &quot;No&quot;,
    &quot;Yes&quot;
)</formula>
    <label>Has a credit check been completed?</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
