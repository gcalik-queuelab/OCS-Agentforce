<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>YTD_Current_Month__c</fullName>
    <description>True where the Opportunity Award Date is the current Year and Month</description>
    <formula>AND(
    YEAR(CloseDate) = YEAR(TODAY()),
    MONTH(CloseDate) &lt;= MONTH(TODAY())
)</formula>
    <label>YTD Current Month</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
