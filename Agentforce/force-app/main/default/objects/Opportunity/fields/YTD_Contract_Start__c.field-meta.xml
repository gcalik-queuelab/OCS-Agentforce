<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>YTD_Contract_Start__c</fullName>
    <description>Identifies Contract has Started in this Year</description>
    <formula>AND(
YEAR(Contract_Start_Date__c) = YEAR(TODAY()),
MONTH(Contract_Start_Date__c) &lt;= MONTH(TODAY())
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>YTD Contract Start</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
