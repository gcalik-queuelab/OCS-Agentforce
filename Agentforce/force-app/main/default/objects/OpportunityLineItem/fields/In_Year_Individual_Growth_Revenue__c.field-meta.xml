<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>In_Year_Individual_Growth_Revenue__c</fullName>
    <formula>IF(YEAR(Opportunity.Contract_Start_Date__c) = YEAR(Opportunity.Contract_End_Date__c),
Individual_Growth__c,
(Individual_Growth__c / 365) * Opportunity.Remaining_Days_In_Year__c
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>In-Year Individual Growth Revenue</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Currency</type>
</CustomField>
